var MAX = 1000;

function start(){
    var present = 1;
    var past = 0;
    var future = 0;
    while (present < MAX){
        println(present);
        future = present + past;
        past = present;
        present = future;
    }
}


==================================================================
C++
==================================================================
#include <iostream>
using namespace std;

const int MAX = 1000;

int main () {
	int present = 1; 
	int past = 0;
	int future = 0;
	while (present < MAX) {
		cout << present;
		future = present + past;
		past = present;
		present = future;
	}
	return 0;
}
