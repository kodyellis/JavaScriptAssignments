JavaScript
++++++++++++++++++++++++++++++++++++++++++++++
var N = 5;

function start(){
    var total = 1;
	for ( var i = N; i > 0;i--){
	   total *= i
	}
	println (total)
}
+++++++++++++++++++++++++++++++++++++++++++++++

C++
+++++++++++++++++++++++++++++++
#include <iostream>
using namespace std;

int main () {
	int total = 1;
	int N = 5;
	for (int i = N; i > 0; i--) {
		total *= i;
	}
	
	return 0;
}
